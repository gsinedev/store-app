<section>
  <div class="container mt-5">
    <div class="row" style="display: grid; place-items: center">
      <div class="col-md-6 d-flex flex-column">
        <div class="align-self-center ubuntu">
          <h3>Discover anything</h3>

          <hr />
        </div>
        <div class="mb-3">
          <form action="" method="">
            <div class="input-group mb-3">
              <input
                type="text"
                class="form-control ubuntu"
                name="search"
                placeholder="Type Something Amazing"
                aria-label="Type Something Amazing"
                aria-describedby="button-addon2"
              />
              <button class="btn bg-orange text-light poppin" type="button" id="button-addon2">Search</button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</section>

<section>
  <div class="container mt-5 mb-5">
    <div class="text-center ubuntu">
      <h1>Products</h1>
    </div>
    <hr />
  </div>
</section>

<section id="product-section">
  <div class="container">
    <div class="row">
      <% items.forEach((element, index) => { %> <%- include('./partials/itemcard.ejs', {element: element, index}) %> <%
      }) %>
    </div>
  </div>
</section>

<script>
  document.getElementsByClassName("item-card-1").remove;
</script>
